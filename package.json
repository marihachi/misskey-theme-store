{
  "private": true,
  "name": "misskey-theme-store",
  "version": "0.1.0",
  "description": "The theme store of the Misskey",
  "main": "./built/index.js",
  "scripts": {
    "build": "npm run build.server && npm run build.client",
    "build.dev": "npm run build.server && npm run build.client.dev",
    "build.server": "tsc -p ./server",
    "build.client": "npm run build.client.webpack && npm run build.client.pages",
    "build.client.dev": "npm run build.client.webpack.dev && npm run build.client.pages",
    "build.client.webpack": "webpack --config \"client/webpack.config.js\" --mode=production",
    "build.client.webpack.dev": "webpack --config \"client/webpack.config.js\" --mode=development",
    "build.client.pages": "cpx ./server/src/app/views/* ./built/app/views",
    "start": "node ./",
    "init": "node ./ --init",
    "clean": "rm -rf ./built"
  },
  "author": "marihachi",
  "contributors": [],
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.x",
    "@fortawesome/free-solid-svg-icons": "5.8.x",
    "@fortawesome/vue-fontawesome": "0.1.6",
    "@types/argv": "0.0.4",
    "@types/crypto-random-string": "1.0.x",
    "@types/express": "4.16.x",
    "@types/json5": "0.0.30",
    "@types/mongodb": "3.1.x",
    "@types/node": "11.11.x",
    "argv": "^0.0.2",
    "body-parser": "1.18.x",
    "cafy": "12.0.x",
    "cpx": "1.5.x",
    "crypto-random-string": "1.0.x",
    "css-loader": "2.1.x",
    "express": "4.16.x",
    "json5": "2.1.x",
    "moment": "2.24.x",
    "mongodb": "3.2.x",
    "node-sass": "4.11.x",
    "normalize.css": "8.0.x",
    "pug": "2.0.x",
    "pug-cli": "^1.0.0-alpha6",
    "sass-loader": "7.1.x",
    "style-loader": "0.23.x",
    "ts-loader": "5.3.x",
    "typescript": "^3.3.3333",
    "vue": "2.6.x",
    "vue-class-component": "7.0.x",
    "vue-loader": "15.7.x",
    "vue-property-decorator": "8.1.x",
    "vue-router": "3.0.x",
    "vue-style-loader": "4.1.x",
    "vue-template-compiler": "2.6.x",
    "vuex": "3.1.x",
    "vuex-module-decorators": "^0.9.8",
    "webpack": "4.29.x",
    "webpack-cli": "3.3.x"
  }
}
